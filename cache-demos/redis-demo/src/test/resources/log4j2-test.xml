<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="5" status="trace">
	<Properties>
	</Properties>
	<Appenders>
		<Console name="STD_OUT" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{ABSOLUTE} %nano [%t] [%-5level] [%class{-3}] [%marker{1}] - %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="STD_OUT" />
		</Root>
	</Loggers>
</Configuration>
